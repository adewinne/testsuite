{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://therefore.ca/contact"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div.col-sm-6"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "name"
      },
      "text": "test"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "text": "adewinne@gmail.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "comment"
      },
      "text": "test message"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='contact-form']//button[.='Email us  ']"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}